    fontFamily: \"Arial\",\n    fontWeight: 500,\n    color: '#0088cc'\n  },\n  category: {\n    color: '#005580',\n    fontWeight: 600,\n  },\n  resumeButton: {\n    borderColor: \"#005580\",\n    color: '#005580'\n  },\n  card: {\n    display: 'flex',\n  },\n  cardDetails: {\n    flex: 1,\n  },\n  cardMedia: {\n    width: 160,\n  },\n});\n\nexport default function MyCourseCard(props) {\n  const classes = useStyles();\n  const { course } = props;\n  const path = \"/courses/\" + course._id;\n  const preventDefault = (event) => event.preventDefault();\n\n  return (\n    <Grid item xs={12}>\n      <Card className={classes.card}>\n        <div className={classes.cardDetails}>\n          <CardContent>\n            <Typography component=\"h2\" variant=\"h5\">\n              <Link underline='none' style={{ color: '#005580' }} className={classes.title} component={RouterLink} to={path}>\n                <TitleStyled>\n                  {course.title}\n                </TitleStyled>\n              </Link>\n            </Typography>\n            <Grid container direction=\"row\" style={{ marginTop: 10 }}>\n              <Grid item container>\n                <Typography variant=\"subtitle2\" color=\"textSecondary\" style={{ flexGrow: 1 }}>\n                  {course.leturer}\n                </Typography>\n                <Typography variant=\"subtitle2\">\n                  <Link component={RouterLink} to={`/categories/${course.category}`} style={{ textDecoration: 'none' }} className={classes.category}>\n                    {course.category}\n                  </Link>\n                </Typography>\n              </Grid>\n              <Grid item>\n                <Typography variant=\"subtitle2\" color=\"textSecondary\">\n                  Last accessed - {course.last_updated}\n                </Typography>\n              </Grid>\n            </Grid>\n            <Typography variant=\"subtitle1\" style={{ marginTop: 10 }}>\n              <DescriptionStyled>\n               