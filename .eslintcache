lWidth>\n          <Grid container justify=\"flex-start\" alignItems=\"center\">\n            <input\n              accept=\"image/*\"\n              className={classes.input}\n              id=\"contained-button-file\"\n              multiple\n              type=\"file\"\n              onChange={this.handleUploadClick}\n            />\n            <label htmlFor=\"contained-button-file\">\n              <Fab component=\"span\" className={classes.button}>\n                <AddPhotoAlternateIcon />\n              </Fab>\n            </label>\n          </Grid>\n        </CardContent>\n      </React.Fragment>\n    );\n  }\n\n  renderUploadedState() {\n    const { classes, theme } = this.props;\n\n    return (\n      <React.Fragment>\n        <CardActionArea onClick={this.imageResetHandler}>\n          <img\n            width=\"100%\"\n            className={classes.media}\n            src={this.state.selectedFile}\n          />\n        </CardActionArea>\n      </React.Fragment>\n    );\n  }\n\n  imageResetHandler = event => {\n    console.log(\"Click!\");\n    this.setState({\n      mainState: \"initial\",\n      selectedFile: null,\n      imageUploaded: 0\n    });\n  };\n\n  render() {\n    const { classes, theme } = this.props;\n\n    return (\n      <React.Fragment>\n        <div className={classes.root}>\n          <Card className={this.props.cardName}>\n            {(this.state.mainState === \"initial\" && this.renderInitialState()) ||\n              (this.state.mainState === \"uploaded\" &&\n                this.renderUploadedState())}\n          </Card>\n        </div>\n      </React.Fragment>\n    );\n  }\n}\n\nexport default withStyles(styles, { withTheme: true })(ImageUploadCard);","/Users/nauqmac/Documents/WEB/WenNC-Project-Frontend/src/components/Home/Carousel/index.js",[],"/Users/nauqmac/Documents/WEB/WenNC-Project-Frontend/src/layouts/AdminLayout/SideNavBar/index.js",["201","202"],"import React, { useEffect } from \"react\";\nimport { Link as RouterLink