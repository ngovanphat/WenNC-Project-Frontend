 {\n    console.log();\n    var file = event.target.files[0];\n    const reader = new FileReader();\n    var url = reader.readAsDataURL(file);\n\n    reader.onloadend = function (e) {\n      this.setState({\n        selectedFile: [reader.result]\n      });\n    }.bind(this);\n    console.log(url); // Would see a path?\n\n    this.setState({\n      mainState: \"uploaded\",\n      selectedFile: event.target.files[0],\n      imageUploaded: 1\n    });\n  };\n\n  renderInitialState() {\n    const { classes, theme } = this.props;\n    const { value } = this.state;\n\n    return (\n      <React.Fragment>\n        <CardContent fullWidth>\n          <Grid container justify=\"flex-start\" alignItems=\"center\">\n            <input\n              accept=\"image/*\"\n              className={classes.input}\n              id=\"contained-button-file\"\n              multiple\n              type=\"file\"\n              onChange={this.handleUploadClick}\n            />\n            <label htmlFor=\"contained-button-file\">\n              <Fab component=\"span\" className={classes.button}>\n                <AddPhotoAlternateIcon />\n              </Fab>\n            </label>\n          </Grid>\n        </CardContent>\n      </React.Fragment>\n    );\n  }\n\n  renderUploadedState() {\n    const { classes, theme } = this.props;\n\n    return (\n      <React.Fragment>\n        <CardActionArea onClick={this.imageResetHandler}>\n          <img\n            width=\"100%\"\n            className={classes.media}\n            src={this.state.selectedFile}\n          />\n        </CardActionArea>\n      </React.Fragment>\n    );\n  }\n\n  imageResetHandler = event => {\n    console.log(\"Click!\");\n    this.setState({\n      mainState: \"initial\",\n      selectedFile: null,\n      imageUploaded: 0\n    });\n  };\n\n  render() {\n    const { classes, theme } = this.props;\n\n    return (\n      <React.Fragment>\n        <div className={classes.root}>\n          <Card className={this.props.cardName}>\n            {(this.state.mainState === \"initial\" && this.renderInitialState()) ||\n              (this.state.mainState === \"uploaded\" &&\n                this.renderUploadedState())}\n          </Card>\n        </div>\n      </React.Fragment>\n    );\n  }\n}\n\nexport default withStyles(styles, { withTheme: true })(ImageUploadCard);","/Users/nauqmac/Documents/WEB/WenNC-Project-Frontend/src/components/CourseDetail/VideoList/index.js",[],"/Users/nauqmac/Documents/WEB/WenNC-Project-Frontend/src/helpers/baseUrl.js",[],"/Users/nauqmac/Documents/WEB/WenNC-Project-Frontend/src/components/Home/TopCategoriesContainer/index.js",["325"],"import React from 'react';\nimport { Link as RouterLink } from 'react-router-dom';\nimport { Grid, Typography, Hidden, Link, GridList, GridListTile } from '@material-ui/core';\nimport { fade, makeStyles } from '@material-ui/core/styles';\nimport CategoryCard from './CategoryCard';\n\nconst useStyles = makeStyles((theme) => ({\n  wrapper: {\n    width: '100%',\n    margin: 20,\n    alignItems: 'center',\n    justify: 'center'\n  },\n  lable: {\n    flexGrow: '1',\n    fontSize: 25,\n    fontWeight: 700\n  },\n  showMore: {\n    textDecoration: 'none',\n    color: '#005580',\n    '&:hover': {\n      color: fade(theme.palette.info.light, 1),\n    }\n  }\n}));\n\nconst TopCategoriesContainer = (props) => {\n  const classes = useStyles();\n\n  const categoryList = props.hotCategories.map((category) => {\n    return (\n      <GridListTile key={category._id} cols={2}>\n        <CategoryCard data={category} />\n      </GridListTile>\n    );\n  });\n\n  if (props.hotCategoriesLoading) {\n    return (\n      <Grid container>\n        <Grid item row xs={12}>\n          <Typography variant=\"h4\">Loading....</Typography>\n        </Grid>\n      </Grid>\n    );\n  }\n  else if (props.hotCategoriesErrMess) {\n    return (\n      <Grid container>\n        <Grid item row xs={12}>\n          <Typography variant=\"h4\">{props.hotCategoriesErrMess}